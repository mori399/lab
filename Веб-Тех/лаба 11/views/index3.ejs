<body>

  <script>
    function printArrayElements(N) {
      const arr = [];
      for (let i = 0; i < N; i++) { 			
        arr.push(i);
      }
      
      for (let i = 0; i < N; i++) { 			
        arr.push(i);

        setTimeout(
          () => { console.log(`Item: ${arr[i]}`); }, 
          (i + 1 ) * 3000);
      }
    }

    var n = prompt("Please, input the number: ");
    printArrayElements(n);
  </script>

</body>
</html>